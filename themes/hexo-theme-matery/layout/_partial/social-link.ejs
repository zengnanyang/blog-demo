<% if (theme.socialLink.qq) { %>
    <a href="tencent://AddContact/?fromId=50&fromSubId=1&subcmd=all&uin=<%= theme.socialLink.qq %>" class="social-img" target="_blank">
        <i class="fab fa-qq"></i>
        <span class="qq-pop">
            <img class="qq-img" src="<%= url_for('img/1144022459.png') %>" width="120" height="120" alt="qq">
        </span>
    </a>
<% } %>

<% if (theme.socialLink.wechat) { %>
    <a href="javascript: void(0);" class="social-img" target="_blank">
        <i class="fab fa-weixin"></i>
        <span class="wechat-pop">
            <img class="wechat-img" src="<%= url_for('img/wechat.png') %>" width="120" height="120" alt="wechat">
        </span>
    </a>
<% } %>

